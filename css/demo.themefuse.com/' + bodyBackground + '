<!-- Add Background To Body -->
<script>
	jQuery(document).ready(function ($) {
		var bodyBackground = 'https://themefuse.com/the-core-multi-purpose-wordpress-theme/images/header-the-core.jpg';
		$('body').append('<img src="'&#32;+&#32;bodyBackground&#32;+&#32;'" alt="" class="testimage hidden">');

		$('.testimage').on('load', function () {
			$('body').css('background-image', 'url(' + bodyBackground + ')');
		});
	});
</script>

